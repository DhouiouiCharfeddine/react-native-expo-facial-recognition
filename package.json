{
  "name": "react-native-facial-recognition",
  "version": "1.0.0",
  "description": "A complete React Native facial recognition app using ArcFace ONNX model with registration, verification, and persistent storage capabilities",
  "main": "index.ts",
  "author": {
    "name": "Mateus",
    "url": "https://github.com/maateusx"
  },
  "license": "MIT",
  "homepage": "https://github.com/maateusx/react-native-facial-recognition#readme",
  "repository": {
    "type": "git",
    "url": "https://github.com/maateusx/react-native-facial-recognition.git"
  },
  "bugs": {
    "url": "https://github.com/maateusx/react-native-facial-recognition/issues"
  },
  "keywords": [
    "react-native",
    "facial-recognition",
    "face-detection",
    "arcface",
    "onnx",
    "machine-learning",
    "computer-vision",
    "expo",
    "typescript",
    "biometric",
    "authentication",
    "face-verification"
  ],
  "engines": {
    "node": ">=18.0.0",
    "npm": ">=8.0.0"
  },
  "scripts": {
    "start": "expo start",
    "dev": "expo start --dev-client",
    "android": "expo run:android",
    "ios": "expo run:ios",
    "web": "expo start --web",
    "build": "expo prebuild && npx pod-install && expo start --dev-client",
    "build:android": "expo build:android",
    "build:ios": "expo build:ios",
    "prebuild": "expo prebuild --clean",
    "lint": "eslint . --ext .ts,.tsx,.js,.jsx",
    "type-check": "tsc --noEmit",
    "clean": "expo r -c && watchman watch-del-all && rm -rf node_modules && npm install",
    "test": "jest",
    "postinstall": "npx pod-install"
  },
  "dependencies": {
    "@react-native-async-storage/async-storage": "2.1.2",
    "@shopify/react-native-skia": "1.5.0",
    "@tensorflow/tfjs": "^4.22.0",
    "@tensorflow/tfjs-react-native": "^1.0.0",
    "expo": "~53.0.20",
    "expo-asset": "~11.1.7",
    "expo-camera": "~16.1.11",
    "expo-dev-client": "~5.2.4",
    "expo-gl": "~15.1.7",
    "expo-image-manipulator": "~13.1.7",
    "expo-image-picker": "^16.1.4",
    "expo-status-bar": "~2.2.3",
    "onnxruntime-react-native": "^1.22.0",
    "react": ">=18.0.0",
    "react-native": ">=0.70.0",
    "react-native-fs": "^2.20.0",
    "react-native-reanimated": "~3.17.4",
    "react-native-vision-camera": "4.6.4",
    "react-native-vision-camera-face-detector": "1.7.2",
    "react-native-worklets-core": "^1.6.0",
    "expo-clipboard": "~7.1.5"
  },
  "devDependencies": {
    "@babel/core": "^7.25.2",
    "@types/react": "~19.0.10",
    "@typescript-eslint/eslint-plugin": "^6.0.0",
    "@typescript-eslint/parser": "^6.0.0",
    "eslint": "^8.0.0",
    "eslint-plugin-react": "^7.33.0",
    "eslint-plugin-react-native": "^4.1.0",
    "jest": "^29.7.0",
    "prettier": "^3.0.0",
    "typescript": "~5.8.3"
  },
  "peerDependencies": {
    "react": ">=18.0.0",
    "react-native": ">=0.70.0"
  },
  "private": false,
  "publishConfig": {
    "access": "public"
  }
}
